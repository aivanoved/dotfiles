{
  "codeLens.enable": true,
  "codeLens.position": "top",
  "diagnostic.displayByAle": true,
  "diagnostic.floatConfig": {
    "border": true
  },
  "hover.floatConfig": {
    "border": true
  },
  "lightbulb.enableSign": true,
  "pydocstring.formatter": "numpy",
  "python.pythonPath": "~/.vim/coc-pyright-conda.sh",
  "pyright.testing.provider": "pytest",
  "signature.floatConfig": {
    "border": true
  },
  "signature.hideOnTextChange": true,
  "suggest.autoTrigger": "none",
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.noselect": true,
  "suggest.virtualText": true
}
